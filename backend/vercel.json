{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    {
      "handle": "router",
      "src": "https://sellerkin-keywordvolume.vercel.app/api/(.*)",
      "method": ["GET", "POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
