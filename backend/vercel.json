{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    { "src": "/api/getkeywords", "dest": "./Controllers/getKeywords.js" },
    { "src": "/api/setKeyword", "dest": "./Controllers/setKeywords.js" },
    { "src": "/(.*)", "dest": "server.js" }
  ]
}
